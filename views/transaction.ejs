<%- include ('header'); -%>
<section id="customer">

    <div class="customerTable table-responsive-sm">
        <table class="table table-striped text-center table-hover">
            <thead>
                <tr>
                    
                    <th>FROM</th>
                    <th>TO</th>
                    <th>AMOUNT</th>
                    <th>TIME</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <% newData.forEach(function(item){ %>
                        <td><%= item.from %> </td>
                        <td><%= item.to %></td>
                        <td><%= item.amount %></td>
                        <td><%= item.ts %></td>
                        </tr>
                    <%});%>


            </tbody>
        </table>
    </div>
</section>

