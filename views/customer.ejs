<%- include ('header'); -%>
    <section id="customer">

        <div class="customerTable table-responsive-sm">
            <table class="table table-striped  table-hover text-center ">
                <thead>
                    <tr>
                        <th class="subheading">NAME</th>
                        <th class="subheading">EMAIL</th>
                        <th class="subheading">BALANCE</th>
                        <th class="subheading">ACTION</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <% newData.forEach(function(item){ %>
                            <td><%= item.name %> </td>
                            <td><%= item.emailId %></td>
                            <td><%= item.currentbalance %></td>
                            <td><a href="/customer/<%=item.name%>"><button class="btn" type="button">TRANSFER</button></a></td>
                            </tr>
                        <%});%>


                </tbody>
            </table>
        </div>
    </section>

